{{ form_start(form) }}
<div class="container">
	<div class="row">
		<div class="col-2"></div>
		<div class="col-8">
			<div class="card">
				<div class="card-body mx-auto d-flex flex-column">

					<div>
						<p>Catégories</p>
						<div class="mt-3 d-flex flex-column w-75 mx-auto">
							<p class="p-black">Catégories
								{{ form_widget(form.category) }}</p>
							{{ form_widget(form.imageFile) }}
							<p class="p-black">Nom du projet
								{{ form_widget(form.title) }}</p>
							{{ form_widget(form.date) }}
							<p class="p-black">Thème
								{{ form_widget(form.theme) }}</p>
							<p class="p-black">Liens
								{{ form_widget(form.link) }}</p>
							<p class="p-black">Outils
								{{ form_widget(form.stack) }}</p>
							{{ form_widget(form.description) }}
							{{ form_widget(form) }}
						</div>
						{{ form_end(form) }}
					</div>
					<div class="text-center" style="height: 100px;">
						<button class="btn-new">{{ button_label|default('Save') }}</button>
						<button class="btn-secondary" href="{{ path('app_crud_index') }}">Retour à la liste</button>
					</div>
					<div class="col-2"></div>
				</div>
			</div>
		</div>
